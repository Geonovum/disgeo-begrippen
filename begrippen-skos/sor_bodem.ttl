sor_begrip:Zand rdf:type skos:Concept;
            rdfs:label "Zand"@nl;
            skos:prefLabel "Zand"@nl;
            skos:definition "Type gedetailleerd van verhardingsobject, onverhard  waarvan het oppervlak (deklaag) bestaat uit zand."@nl;
            skos:broader sor_begrip:Bodem;
            skos_thes:broaderGeneric sor_begrip:Bodem;
            skos:inScheme begrippenkaders:sor ;
            . 
sor_begrip:ZwarteGrond rdf:type skos:Concept;
            rdfs:label "Zwarte grond"@nl;
            skos:prefLabel "Zwarte grond"@nl;
            skos:definition "Type gedetailleerd van verhardingsobject, waarvan het oppervlak bestaat uit zwarte grond."@nl;
            skos:broader sor_begrip:Bodem;
            skos_thes:broaderGeneric sor_begrip:Bodem;
            skos:inScheme begrippenkaders:sor ;
            . 
sor_begrip:OpenGrond rdf:type skos:Concept;
            rdfs:label "Open grond"@nl;
            skos:prefLabel "Open grond"@nl;
            skos:definition "Type gedetailleerd van verhardingsobject, onverhard waarvan het oppervlak bestaat uit open grond, niet zijnde zand of zwarte grond."@nl;
